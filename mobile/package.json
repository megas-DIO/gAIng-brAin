{
  "name": "omega-mobile",
  "version": "1.0.0",
  "description": "OMEGA Brain Mobile Application",
  "main": "dist/index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "cap:init": "cap init 'OMEGA Brain' com.gaing.omega --web-dir dist",
    "cap:add:android": "cap add android",
    "cap:add:ios": "cap add ios",
    "cap:sync": "cap sync",
    "cap:open:android": "cap open android",
    "cap:open:ios": "cap open ios",
    "android": "npm run build && cap sync android && cap open android",
    "ios": "npm run build && cap sync ios && cap open ios",
    "build:android": "npm run build && cap sync android && cd android && ./gradlew assembleRelease",
    "build:ios": "npm run build && cap sync ios"
  },
  "dependencies": {
    "@capacitor/android": "^5.6.0",
    "@capacitor/app": "^5.0.6",
    "@capacitor/core": "^5.6.0",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "^5.6.0",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/push-notifications": "^5.1.0",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@capacitor/preferences": "^5.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.6.0",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.10"
  },
  "keywords": [
    "omega",
    "mobile",
    "capacitor",
    "ios",
    "android"
  ],
  "author": "gAIng Collective",
  "license": "MIT"
}
